       </el-select>\n      </el-form-item>\n\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onSubmit\">Create</el-button>\n        <el-button type=\"primary\" @click=\"onGet\">Get</el-button>\n        <el-button type=\"primary\" @click=\"onUpdate\">Update</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n  <div class=\"get\" v-if=\"isGot == true\">\n    <div>{{ form.id }}</div>\n    <div>{{ form.name }}</div>\n    <div>{{ form.desc }}</div>\n    <div>{{ form.mobile }}</div>\n    <div>{{ form.email }}</div>\n    <div>{{ form.wechat }}</div>\n    <div>{{ form.exclusive }}</div>\n  </div>\n  <el-upload\n      :http-request=\"uploadFile\"\n      :file-list=\"file\"\n      class=\"upload-demo\"\n      action\n      :limit=\"1\"\n      :disabled=\"this.isLimited\"\n      @click=\"onAvator\"\n  >\n    <el-button type=\"primary\">Avator upload</el-button>\n    <template #tip>\n      <div class=\"el-upload__tip\">\n        jpg/png files with a size less than 500KB. Limit: 1.\n      </div>\n    </template>\n  </el-upload>\n  <el-upload\n      :http-request=\"uploadFile\"\n      :file-list=\"fileList\"\n      class=\"upload-demo\"\n      action\n      multiple\n      :limit=\"3\"\n      @click=\"onHonor\"\n  >\n    <el-button type=\"primary\" @click=\"onHonor\">Honor upload</el-button>\n    <template #tip>\n      <div class=\"el-upload__tip\">\n        jpg/png files with a size less than 500KB.\n      </div>\n    </template>\n  </el-upload>\n</template>\n\n<script>\nimport { reactive } from 'vue';\nimport store from '@/vuex/store';\nimport { mapState, mapActions, mapMutations } from 'vuex';\n\nexport default {\n  name: 'App',\n  components: {},\n  data () {\n    return {\n      formdata: '',\n      file: [],\n      fileList: [],\n      isGot: false,\n      isLimited: false,\n      form : reactive({\n        id: 0,\n        name: '',\n        mobile: '',\n        email: '',\n        wechat: '',\n        desc: '',\n        exclusive: 'True',\n      })\n    }\n  },\n  computed: {\n    ...mapState(['Provider', 'fid'])\n  },\n  methods: {\n    ...mapActions(['pro_create', 'pro_get', 'pro_update']),\n    ...mapMutations(['SET_PROVIDER']),\n    onSubmit () {\n      this.pro_create(this.form);\n    },\n    onGet () {\n      console.log(this.form);\n      this.form.id = this.fid;\n      this.pro_get(this.form.id);\n      this.isGot = true;\n    },\n    onUpdate () {\n      this.pro_update(this.form); // 仅更新最初提交的id的数据\n    },\n    onAvator () {\n      this.isLimited = true;\n      this.formdata = new FormData;\n      this.$refs.upload.submit();\n      this.$axios.post\n    },\n    onHonor () {\n\n    },\n    uploadFile(file) {\n      this.formdata.append('file', file.file);\n    }\n  },\n  store\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  div-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n.container {\n  width: 100%;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.get {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}