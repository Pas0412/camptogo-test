{"ast":null,"code":"import { reactive } from 'vue';\nimport store from '@/vuex/store';\nimport { mapState, mapActions, mapMutations } from 'vuex';\nexport default {\n  name: 'App',\n  components: {},\n\n  data() {\n    return {\n      formdata: '',\n      file: [],\n      fileList: [],\n      isGot: false,\n      isLimited: false,\n      form: reactive({\n        id: 0,\n        name: '',\n        mobile: '',\n        email: '',\n        wechat: '',\n        desc: '',\n        exclusive: 'True'\n      })\n    };\n  },\n\n  computed: { ...mapState(['Provider', 'fid'])\n  },\n  methods: { ...mapActions(['pro_create', 'pro_get', 'pro_update']),\n    ...mapMutations(['SET_PROVIDER']),\n\n    onSubmit() {\n      this.pro_create(this.form);\n    },\n\n    onGet() {\n      console.log(this.form);\n      this.form.id = this.fid;\n      this.pro_get(this.form.id);\n      this.isGot = true;\n    },\n\n    onUpdate() {\n      this.pro_update(this.form); // 仅更新最初提交的id的数据\n    },\n\n    onAvator() {\n      this.isLimited = true;\n      this.formdata = new FormData();\n      this.$refs.upload.submit();\n      this.$axios.post;\n    },\n\n    onHonor() {},\n\n    uploadFile(file) {\n      this.formdata.append('file', file.file);\n    }\n\n  },\n  store\n};","map":{"version":3,"mappings":"AA6EA,SAASA,QAAT,QAAyB,KAAzB;AACA,OAAOC,KAAP,MAAkB,cAAlB;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,YAA/B,QAAmD,MAAnD;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAG,GAAK;IACN,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,IAAI,EAAE,EAFD;MAGLC,QAAQ,EAAE,EAHL;MAILC,KAAK,EAAE,KAJF;MAKLC,SAAS,EAAE,KALN;MAMLC,IAAG,EAAIb,QAAQ,CAAC;QACdc,EAAE,EAAE,CADU;QAEdT,IAAI,EAAE,EAFQ;QAGdU,MAAM,EAAE,EAHM;QAIdC,KAAK,EAAE,EAJO;QAKdC,MAAM,EAAE,EALM;QAMdC,IAAI,EAAE,EANQ;QAOdC,SAAS,EAAE;MAPG,CAAD;IANV,CAAP;EAgBD,CApBY;;EAqBbC,QAAQ,EAAE,EACR,GAAGlB,QAAQ,CAAC,CAAC,UAAD,EAAa,KAAb,CAAD;EADH,CArBG;EAwBbmB,OAAO,EAAE,EACP,GAAGlB,UAAU,CAAC,CAAC,YAAD,EAAe,SAAf,EAA0B,YAA1B,CAAD,CADN;IAEP,GAAGC,YAAY,CAAC,CAAC,cAAD,CAAD,CAFR;;IAGPkB,QAA