{"ast":null,"code":"import { reactive } from 'vue';\nimport store from '@/vuex/store';\nimport { mapState, mapActions, mapMutations } from 'vuex';\nexport default {\n  name: 'App',\n  components: {},\n\n  data() {\n    return {\n      isGot: false,\n      isLimited: false,\n      form: reactive({\n        id: 0,\n        name: '',\n        mobile: '',\n        email: '',\n        wechat: '',\n        desc: '',\n        exclusive: 'True'\n      })\n    };\n  },\n\n  computed: { ...mapState(['Provider', 'fid'])\n  },\n  methods: { ...mapActions(['pro_create', 'pro_get', 'pro_update']),\n    ...mapMutations(['SET_PROVIDER']),\n\n    onSubmit() {\n      this.pro_create(this.form);\n    },\n\n    onGet() {\n      console.log(this.form);\n      this.form.id = this.fid;\n      this.pro_get(this.form.id);\n      this.isGot = true;\n    },\n\n    onUpdate() {\n      this.pro_update(this.form); // 仅更新最初提交的id的数据\n    },\n\n    onAvator() {\n      this.isLimited = true;\n    },\n\n    onHonor() {}\n\n  },\n  store\n};","map":{"version":3,"mappings":"AAwEA,SAASA,QAAT,QAAyB,KAAzB;AACA,OAAOC,KAAP,MAAkB,cAAlB;AACA,SAASC,QAAT,EAAmBC,UAAnB,EAA+BC,YAA/B,QAAmD,MAAnD;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,KADF;MAELC,SAAS,EAAE,KAFN;MAGLC,IAAG,EAAIV,QAAQ,CAAC;QACdW,EAAE,EAAE,CADU;QAEdN,IAAI,EAAE,EAFQ;QAGdO,MAAM,EAAE,EAHM;QAIdC,KAAK,EAAE,EAJO;QAKdC,MAAM,EAAE,EALM;QAMdC,IAAI,EAAE,EANQ;QAOdC,SAAS,EAAE;MAPG,CAAD;IAHV,CAAP;EAaD,CAjBY;;EAkBbC,QAAQ,EAAE,EACR,GAAGf,QAAQ,CAAC,CAAC,UAAD,EAAa,KAAb,CAAD;EADH,CAlBG;EAqBbgB,OAAO,EAAE,EACP,GAAGf,UAAU,CAAC,CAAC,YAAD,EAAe,SAAf,EAA0B,YAA1B,CAAD,CADN;IAEP,GAAGC,YAAY,CAAC,CAAC,cAAD,CAAD,CAFR;;IAGPe,QAAO,GAAK;MACV,KAAKC,UAAL,CAAgB,KAAKV,IAArB;IACD,CALM;;IAMPW,KAAI,GAAK;MACPC,OAAO,CAACC,GAAR,CAAY,KAAKb,IAAjB;MACA,KAAKA,IAAL,CAAUC,EAAV,GAAe,KAAKa,GAApB;MACA,KAAKC,OAAL,CAAa,KAAKf,IAAL,CAAUC,EAAvB;MACA,KAAKH,KAAL,GAAa,IAAb;IACD,CAXM;;IAYPkB,QAAO,GAAK;MACV,KAAKC,UAAL,CAAgB,KAAKjB,IAArB,EADU,CACkB;IAC7B,CAdM;;